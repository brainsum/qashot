{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "d3.js": {
            "type": "package",
            "package": {
                "name": "d3-drupal/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/d3/d3",
                    "type": "git",
                    "reference": "v3.5.17"
                }
            }
        },
        "highlightjs": {
            "type": "package",
            "package": {
                "name": "components-drupal/highlightjs",
                "version": "9.12.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/components/highlightjs",
                    "type": "git",
                    "reference": "9.12.0"
                }
            }
        },
        "colorpicker": {
            "type": "package",
            "package": {
                "name": "jaypan/jquery_colorpicker",
                "type": "drupal-library",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/jaypan/jquery_colorpicker",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "composer/installers": "^1.6",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/adminimal_theme": "^1.3",
        "drupal/bootstrap": "^3.13",
        "drupal/core": "^8.6",
        "drupal/inline_entity_form": "^1.0",
        "drupal/jquery_colorpicker": "^1.4",
        "drupal/maxlength": "~1.0",
        "drupal/paragraphs": "^1.3",
        "drush/drush": "^9.5",
        "jaypan/jquery_colorpicker": "^1.0",
        "webflo/drupal-finder": "^1.1",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-goutte-driver": "~1.2",
        "components/highlightjs": "9.7.*",
        "d3/d3": "3.5.*",
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "drupal/admin_toolbar": "^1.24",
        "drupal/adminimal_admin_toolbar": "^1.8",
        "drupal/coder": "^8.3",
        "drupal/console": "^1.8",
        "drupal/cors_ui": "1.x-dev",
        "drupal/devel": "^1.2",
        "drupal/examples": "1.x-dev",
        "drupal/maillog": "1.x-dev",
        "drupal/restui": "^1.15",
        "drupal/security_review": "1.x-dev",
        "drupal/toolbar_anti_flicker": "^2.7",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "^1.6",
        "phpcompatibility/php-compatibility": "^8.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "vendor/bin/phpcs --version && vendor/bin/phpcs --config-set installed_paths vendor/drupal/coder/coder_sniffer,vendor/phpcompatibility/php-compatibility || echo \"PHPCS is not installed\" "
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/security_review": {
                "log() argument is null": "https://www.drupal.org/files/issues/security_review-fatal_error_when_running_the_review-2815487-5.patch"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
